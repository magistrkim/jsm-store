{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier",
    "standart",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "parser": "flow",
        "endOfLine": "auto"
      }
    ]
  },
  "ignorePatterns": ["*.d.ts"]
}
